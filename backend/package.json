{
  "name": "sistema-manutencao-backend",
  "version": "1.0.0",
  "description": "Backend para Sistema de Manutenção",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
