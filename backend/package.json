    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "level": "^10.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.3",
    "pdfkit": "^0.17.1",
    "sequelize": "^6.37.7",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.4"
  }
